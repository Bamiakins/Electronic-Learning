{
  "name": "electronic-learning",
  "version": "1.0.0",
  "description": "A modular and extensible backend for an electronic learning system using Express.js, PostgreSQL, and Prisma. Features include role-based authentication, Google OAuth, custom migrations and seeders, and well-defined service modules.",
  "main": "server.ts",
  "scripts": {
    "test": "jest",
    "start": "node ./dist/server.js",
    "dev": "npx prisma generate && nodemon ./src/server.ts",
    "prisma:studio": "prisma studio",
    "db:migrate": "ts-node prisma/migrations/migration-runner.ts up",
    "db:rollback": "ts-node prisma/migrations/migration-runner.ts down",
    "db:seed": "ts-node prisma/seeders/seeder-runner.ts up",
    "db:seed:undo": "ts-node prisma/seeders/seeder-runner.ts down"
  },
  "keywords": [],
  "author": "Bami Akinfenwa",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "axios": "^1.8.4",
    "bcrypt": "^5.0.0",
    "cloudinary": "^2.6.0",
    "compression": "^1.8.0",
    "config": "^3.3.12",
    "cookie-parser": "^1.4.7",
    "datauri": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.1",
    "multer": "^1.4.5-lts.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.14.1",
    "winston": "^3.17.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.5",
    "@types/config": "^3.3.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.12",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.11.11",
    "@types/winston": "^2.4.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prisma": "^6.5.0",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2"
  }
}
